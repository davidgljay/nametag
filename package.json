  "name": "Nametag",
  "version": "0.0.1",
  "dependencies": {
    "@horizon/client": "2.0.0",
    "bluebird": "^3.4.0",
    "cookie": "^0.2.3",
    "dialog-polyfill": "^0.4.4",
    "fetch": "^1.1.0",
    "jsonwebtoken": "^5.5.4",
    "lodash": "^4.17.4",
    "material-ui": "^0.16.5",
    "mime-types": "^2.0.4",
    "moment": "^2.18.0",
    "oauth": "^0.9.14",
    "pem": "^1.8.1",
    "qrcode.react": "^0.6.1",
    "radium": "^0.18.1",
    "react": "^15.4.0",
    "react-addons-css-transition-group": "^15.3.2",
    "react-addons-update": "^15.4.2",
    "react-dnd": "^2.1.4",
    "react-dnd-touch-backend": "^0.3.3",
    "react-dom": "^15.3.2",
    "react-emoji": "^0.4.4",
    "react-fileupload": "^2.4.0",
    "react-markdown": "^2.4.3",
    "react-onclickoutside": "^5.8.3",
    "react-redux": "5.0.2",
    "react-router": "^2.4.0",
    "react-simple-emoji": "^0.1.3",
    "react-tap-event-plugin": "^2.0.0",
    "react-timeago": "^3.1.3",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "rethinkdb": "^2.1.1",
    "winston": "^2.1.0",
    "ws": "^1.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.16.0",
    "babel-eslint": "^7.0.0",
    "babel-jest": "^14.1.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "copy-webpack-plugin": "^4.0.1",
    "del": "~0.1.3",
    "eslint": "^2.3.0",
    "eslint-plugin-react": "^6.8.0",
    "exports-loader": "^0.6.4",
    "imports-loader": "^0.7.1",
    "istanbul": "^0.4.3",
    "jest-cli": "^18.1.0",
    "joi": "^10.0.1",
    "license-webpack-plugin": "^0.4.3",
    "mocha": "^2.3.3",
    "nodemon": "^1.8.1",
    "pump": "^1.0.1",
    "react-storybook": "^1.0.0",
    "react-test-renderer": "^15.3.1",
    "react-tools": "latest",
    "reactify": "latest",
    "redux-mock-store": "^1.1.2",
    "standard": "^8.6.0",
    "strip-debug": "^1.0.1",
    "style-loader": "^0.13.1",
    "vinyl-source-stream": "^1.0.0",
    "webpack": "^2.2.1"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "jest",
    "test-watch": "jest --watch",
    "start": "docker-compose up -d && gulp watch",
    "build": "NODE_ENV=production webpack --progress -p --config webpack.config.js --bail",
    "build-watch": "NODE_ENV=development webpack --progress --config webpack.config.js --watch",
    "lint": "standard",
    "lint-fix": "standard --fix",
    "backup": "docker-compose run horizon sh backup.sh",
    "syncdb": "docker-compose run horizon hz schema apply schema.toml --project-name nametag --connect rethinkdb",
    "pull": "git pull origin master && npm install && npm run build && docker-compose build horizon && docker-compose stop && docker-compose up -d",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ],
    "testPathIgnorePatterns": [
      "/app/bower_components/",
      "/dist/bower_components",
      "/node_modules/",
      "/dist/",
      "/horizon/"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/app/scripts/tests/mockGlobals.js",
    "automock": true
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      [
        "babelify",
        {
          "ignore": [
            "bower_components"
          ]
        }
      ]
    ]
  },
  "browser": {
    "jquery": "./app/bower_components/jquery/dist/jquery.js"
  },
  "browserify-shim": {
    "jquery": "$"
  },
  "standard": {
    "globals": [
      "describe",
      "it",
      "expect",
      "amplitude",
      "mixpanel",
      "jest",
      "beforeEach",
      "Horizon",
      "fetch",
      "history",
      "self",
      "caches",
      "clients",
      "firebase",
      "importScripts"
    ],
    "ignore": [
      "/node_modules",
      "/horizon"
    ]
  }
}
